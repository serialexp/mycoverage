{
  "name": "mycoverage",
  "version": "1.35.5",
  "scripts": {
    "dev": "blitz dev --port 3002",
    "dev:worker": "nodemon --exec 'npm run build:worker && npm run start:worker' src/worker.ts",
    "build:worker": "webpack --config ./worker.webpack.config.js",
    "start:worker": "DOTENV_CONFIG_PATH=./.env.local node -r dotenv/config ./dist/worker.js",
    "build": "blitz build",
    "build:protobuf": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=esModuleInterop=true --ts_proto_out=./ src/library/proto_coverage.proto",
    "start:frontend": "blitz start",
    "output_version": "awk '/version/{gsub(/(\"|\",)/,\"\",$2);print $2}' package.json",
    "release:publish": "npm run release && docker build -t aeolun/mycoverage:latest -t aeolun/mycoverage:$(npm run output_version) .",
    "start": "node concurrently.js",
    "studio": "blitz prisma studio",
    "migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "test:tsc": "tsc --noEmit",
    "lint": "biome check src && biome format src",
    "lint:fix": "biome lint --apply-unsafe src && biome format --write src",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "housekeeping": "node housekeeping.mjs",
    "healthcheck": "node healthchecks.mjs",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "biome check",
      "biome format"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@smithy/types": "^2.12.0",
    "@blitzjs/auth": "^2.0.5",
    "@blitzjs/next": "^2.0.5",
    "@blitzjs/rpc": "^2.0.5",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@chakra-ui/system": "^2.6.2",
    "@chakra-ui/theme": "^3.3.1",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@nivo/axes": "0.74.1",
    "@nivo/core": "^0.74.1",
    "@nivo/line": "0.74.1",
    "@nivo/tooltip": "^0.74.1",
    "@nivo/treemap": "^0.74.1",
    "@octokit/auth-app": "^4.0.13",
    "@octokit/rest": "^19.0.13",
    "@prisma/client": "^5.11.0",
    "@react-spring/web": "9.7.2",
    "ajv": "8.12.0",
    "axios": "^0.21.4",
    "badge-maker": "^3.3.1",
    "blitz": "^2.0.5",
    "bullmq": "^4.17.0",
    "classnames": "2.3.2",
    "concurrently": "^7.6.0",
    "d3-scale": "4.0.2",
    "d3-scale-chromatic": "^3.1.0",
    "dotenv": "^10.0.0",
    "final-form": "^4.20.10",
    "framer-motion": "^5.6.0",
    "jsonwebtoken": "^8.5.1",
    "long": "5.2.3",
    "memoize-one": "6.0.0",
    "minimist": "^1.2.8",
    "next": "^13.5.6",
    "passport": "0.7.0",
    "passport-github2": "0.1.12",
    "passport-oauth2": "^1.8.0",
    "prismjs": "1.29.0",
    "prop-types": "^15.8.1",
    "protobufjs": "^6.11.4",
    "queue": "^6.0.2",
    "react": "^18.2.0",
    "react-datetime": "^3.2.0",
    "react-diff-viewer-continued": "^4.0.2",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-icons": "^4.12.0",
    "react-timeago": "^7.2.0",
    "uuidv7": "0.6.3",
    "xml2js": "^0.4.23",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.3",
